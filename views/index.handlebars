<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">News Feed
            <small>all news around the world</small>
        </h1>
    </div>
</div>

{{#each posts}}
<div class="row post_row">
    <div class="col-md-12">
        <a href={{url}}>
            <img class="img-thumbnail" src={{"urlToImage"}} alt="" style="width:300px;height:150px;">
        </a>
    <div class="col-md-6">
        <h4><a href={{url}}>{{"title"}}</a></h4>
        <h4><small>News Source : the-hindu</small></h4>
        <h4><small>Author : {{"author"}} Published time: {{"publishedAt"}}</small></h4>
        <p>{{"description"}}</p>
        <a class="btn btn-primary" href={{url}}>Read News <span class="glyphicon glyphicon-chevron-right"></span></a>
    </div>
          </div>
</div>
{{/each}}
